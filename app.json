{
  "expo": {
    "name": "hvac-plumbing-pro-connect",
    "slug": "hvac-plumbing-pro-connect",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/hvac-logo.jpg",
    "scheme": "hvacplumbingproconnectnew",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Allow $(PRODUCT_NAME) to access your photos",
        "NSPhotoLibraryAddUsageDescription": "Allow $(PRODUCT_NAME) to save photos",
        "NSCameraUsageDescription": "Allow $(PRODUCT_NAME) to use your camera",
        "NSMicrophoneUsageDescription": "Allow $(PRODUCT_NAME) to use your microphone"
      },
      "bundleIdentifier": "com.afouche69.hvacplumbingproconnect"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/hvac-logo.jpg",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.afouche69.hvacplumbingproconnect"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/hvac-logo.jpg"
    },
    "plugins": [
      "expo-router",
      "@reactvision/react-viro/app.plugin.js",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/hvac-logo.jpg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "7ee9a041-6511-4aa0-b6d2-5cec848f0894"
      }
    },
    "owner": "afouche69"
  }
}
